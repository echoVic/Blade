{
  "name": "agent-cli",
  "version": "1.0.0",
  "description": "命令行工具",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent": "bin/agent.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "node bin/agent.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "check": "npm run type-check && npm run lint && npm run format:check"
  },
  "keywords": [
    "cli",
    "agent",
    "tool"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.15.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "inquirer": "^12.6.3",
    "openai": "^4.103.0"
  }
}
